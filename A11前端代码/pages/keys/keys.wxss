Page {
  width: 100%;
  height: 100%;
  background: linear-gradient(#c2aeff, #ffffff);
}

.keys-header {
  width: 100%;
  height: 450rpx;
  margin-bottom: 30rpx;
}
.keys-header image {
  width: 100%;
  height: 100%;
}
.keys-textarea {
  width: 100%;
  background-color: #eee;
}
.keys-textarea textarea {
  width: 100%;
}
.keys-place,
.keys-style,
.keys-otherStyle,
.keys-resolution {
  width: 100%;
  margin: 40rpx 0;
}

